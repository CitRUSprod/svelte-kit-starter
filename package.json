{
    "name": "svelte-kit-template",
    "type": "module",
    "scripts": {
        "dev": "svelte-kit dev",
        "build": "svelte-kit build",
        "start": "node dist",
        "validate": "tsc --noEmit && svelte-check",
        "prettier": "prettier --check",
        "prettier:fix": "pnpm prettier -- --write",
        "prettier:all": "pnpm prettier -- \"**/*.{html,md,json,yml,yaml,scss,js,cjs,ts,svelte}\"",
        "prettier:fix:all": "pnpm prettier:all -- --write",
        "stylelint": "stylelint --color",
        "stylelint:fix": "pnpm stylelint -- --fix",
        "stylelint:all": "pnpm stylelint -- **/*.{scss,svelte}",
        "stylelint:fix:all": "pnpm stylelint:all -- --fix",
        "eslint": "eslint --color",
        "eslint:fix": "pnpm eslint -- --fix",
        "eslint:all:scripts": "pnpm eslint -- \"**/*.{js,cjs,ts}\"",
        "eslint:all:svelte": "pnpm eslint -- \"**/*.svelte\"",
        "eslint:all": "pnpm eslint:all:scripts && pnpm eslint:all:svelte",
        "eslint:fix:all": "pnpm eslint:all:scripts -- --fix && pnpm eslint:all:svelte -- --fix",
        "lint": "pnpm prettier:all && pnpm stylelint:all && pnpm eslint:all",
        "format": "pnpm prettier:fix:all && pnpm stylelint:fix:all && pnpm eslint:fix:all"
    },
    "prettier": "./configs/prettier/index.cjs",
    "eslintConfig": {
        "extends": "./configs/eslint/index.cjs"
    },
    "stylelint": {
        "extends": "./configs/stylelint/index.cjs"
    },
    "dependencies": {
        "nprogress": "0.2.0",
        "sveltestrap": "5.0.6"
    },
    "devDependencies": {
        "@citrus-linting/eslint-config": "0.5.0",
        "@citrus-linting/prettier-config": "0.2.0",
        "@citrus-linting/stylelint-config": "0.1.0",
        "@sveltejs/adapter-node": "1.0.0-next.22",
        "@sveltejs/kit": "1.0.0-next.109",
        "@types/nprogress": "0.2.0",
        "@typescript-eslint/eslint-plugin": "4.25.0",
        "@typescript-eslint/parser": "4.25.0",
        "eslint": "7.27.0",
        "eslint-config-prettier": "8.3.0",
        "eslint-plugin-svelte3": "3.2.0",
        "prettier": "2.3.0",
        "prettier-plugin-packagejson": "2.2.11",
        "prettier-plugin-svelte": "2.3.0",
        "sass": "1.34.0",
        "stylelint": "13.13.1",
        "stylelint-config-prettier": "8.0.2",
        "stylelint-scss": "3.19.0",
        "svelte": "3.38.2",
        "svelte-check": "1.5.4",
        "svelte-preprocess": "4.7.3",
        "typescript": "4.2.4"
    }
}
